    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "jest": "^27.4.7",
    "jest-playwright": "^0.0.1",
    "jest-playwright-preset": "^1.4.5",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.22.1",
    "postcss": "^8.4.16",
    "prettier": "^2.6.2",
    "prisma": "^4.3.1",
